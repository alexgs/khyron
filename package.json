{
  "name": "khyron",
  "version": "0.2.0",
  "description": "Run-time type checking for JavaScript",
  "main": "index.js",
  "author": {
    "name": "Phil Gates-Shannon",
    "email": "phil.gs@gmail.com",
    "url": "http://philgs.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philgs/khyron.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.12.0"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "npm run build && mocha test/unit-tests.js",
    "build": "babel src -d dist",
    "test:imp": "npm run build && mocha test/implementation.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.12.0"
          }
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "exedore": "^1.0.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "mocha": "^5.0.1",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0"
  }
}
